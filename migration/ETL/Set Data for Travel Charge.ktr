  <step>
    <name>Read Data for Travel Charge from Adempiere </name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Adempiere</connection>
    <sql>SELECT lo.AD_Org_ID,rw.outdate AS dfecha_carga,et.fta_vehicle_id,
et.fta_driver_id, 0 AS isprocessed, rw.fta_recordweight_id,
coalesce (rw.netweight,lo.weight) netweight, 
lo.documentno  as nid_guia
FROM fta_entryticket et 
INNER JOIN fta_loadorder lo ON et.fta_entryticket_id = lo.fta_entryticket_id 
left JOIN fta_recordweight rw ON lo.fta_loadorder_id = rw.fta_loadorder_id  
WHERE et.AD_Client_ID = ${COMPANY} and lo.docstatus in(&apos;CO&apos;, &apos;CL&apos;) and lo.isDelivered = &apos;Y&apos; </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
