  <step>
    <name>Read Data from Adempiere for Cash Document</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Adempiere</connection>
    <sql>SELECT * FROM 
(SELECT (SELECT ad_table_id FROM ad_table WHERE tablename=&apos;C_Invoice&apos;) AS ad_table_id,c_invoice_id AS record_id,documentno,controlno,dateinvoiced AS datedoc,
grandtotal,&apos;I&apos; AS recordtype, 0 AS isprocessed, bp.taxid, i.ad_org_id, i.user1_id as ncentro
FROM c_invoice i 
INNER JOIN c_bpartner bp ON i.c_bpartner_id = bp.c_bpartner_id 
WHERE issotrx=&apos;N&apos; AND docstatus = &apos;CO&apos; AND i.ad_client_id = ${COMPANY}
UNION ALL
SELECT (SELECT ad_table_id FROM ad_table WHERE tablename=&apos;C_CashLine&apos;) AS ad_table_id,c_cashline_id AS record_id,cl.referenceno AS documentno,controlno,datedoc,
amount*-1 AS grandtotal,&apos;C&apos; AS recordtype, 0 AS isprocessed, bp.taxid, cl.ad_org_id, c.user1_id as ncentro
FROM c_cash c 
INNER JOIN c_cashline cl ON c.c_cash_id=cl.c_cash_id 
INNER JOIN c_bpartner bp ON cl.c_bpartner_id = bp.c_bpartner_id 
WHERE c.docstatus=&apos;CO&apos; AND cashtype = &apos;C&apos; AND cl.ad_client_id = ${COMPANY} 
UNION ALL 
SELECT (SELECT ad_table_id FROM ad_table WHERE tablename=&apos;M_Inventory&apos;) AS ad_table_id,i.m_inventory_id AS record_id,i.documentno,&apos;&apos; AS controlno,i.movementdate AS datedoc,
SUM(cd.currentcostprice) AS grandtotal,&apos;M&apos; AS recordtype, 0 AS isprocessed, oi.taxid, i.ad_org_id, i.user1_id as ncentro 
FROM m_inventory i 
INNER JOIN m_inventoryline il ON i.m_inventory_id = il.m_inventory_id 
INNER JOIN m_costdetail cd ON il.m_inventoryline_id = cd.m_inventoryline_id 
INNER JOIN ad_orginfo oi ON i.ad_org_id = oi.ad_org_id 
WHERE i.docstatus=&apos;CO&apos; AND i.isinternal = &apos;Y&apos; AND i.ad_client_id = ${COMPANY}
GROUP BY (SELECT ad_table_id FROM ad_table WHERE tablename=&apos;M_Inventory&apos;),i.m_inventory_id,i.documentno,i.movementdate,oi.taxid) records where ncentro = 1000396
ORDER BY ad_org_id 
</sql>
